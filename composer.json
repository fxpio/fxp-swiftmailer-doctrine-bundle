{
    "name": "sonatra/swiftmailer-doctrine-bundle",
    "type": "symfony-bundle",
    "description": "Doctrine spool for Swiftmailer",
    "keywords": ["swiftmailer", "spool", "doctrine"],
    "homepage": "https://github.com/sonatra/SonatraSwiftmailerDoctrineBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "FranÃ§ois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/framework-bundle": "~3.0",
        "symfony/console": "~3.0",
        "symfony/swiftmailer-bundle": "~2.4",
        "sonatra/swiftmailer-doctrine": "~1.0@dev"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "~1.6"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Bundle\\SwiftmailerDoctrineBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
