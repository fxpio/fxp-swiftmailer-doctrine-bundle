{
    "name": "sonatra/swiftmailer-doctrine-bundle",
    "type": "symfony-bundle",
    "description": "Doctrine spool for Swiftmailer",
    "keywords": ["swiftmailer", "spool", "doctrine"],
    "homepage": "https://github.com/sonatra/sonatra-swiftmailer-doctrine-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "symfony/framework-bundle": "^3.4.0|^4.0.0",
        "symfony/console": "^3.4.0|^4.0.0",
        "symfony/swiftmailer-bundle": "^3.1.0",
        "sonatra/swiftmailer-doctrine": "^1.0.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.8.0"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Bundle\\SwiftmailerDoctrineBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
